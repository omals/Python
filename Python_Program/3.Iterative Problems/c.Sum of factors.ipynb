      "source": [
        "n=int(input(\"Enter the number:\"))\n",
        "sum=0\n",
        "for i in range(1,n+1):\n",
        "  if n%i==0:\n",
        "    sum=sum+i\n",
        "print(\"Sum of Factors of \",n,\" is \",sum)\n"
