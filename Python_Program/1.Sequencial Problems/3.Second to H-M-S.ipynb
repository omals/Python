      "source": [
        "time=int(input(\"Enter the second to convert: \"))\n",
        "hour=int(time/3600)\n",
        "min=int((time-(hour*3600))/60)\n",
        "sec=int(time-((hour*3600)+(min*60)))\n",
        "print(\"Hour= \",hour,\": Minute= \",min,\": Second= \",sec)"
